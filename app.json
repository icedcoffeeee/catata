{
  "expo": {
    "name": "catata",
    "slug": "catata",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.icedcoffeeee.catata"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "datePicker": {
              "colorAccent": {
                "light": "#ff3b82f6",
                "dark": "#ff3b82f6"
              },
              "colorControlActivated": {
                "light": "#ff3b82f6",
                "dark": "#ff3b82f6"
              },
              "colorControlHighlight": {
                "light": "#ffe4e4e7",
                "dark": "#ff27272a"
              },
              "selectableItemBackgroundBorderless": {
                "light": "#ffe4e4e7",
                "dark": "#ff27272a"
              },
              "textColor": {
                "light": "#ff18181b",
                "dark": "#fff4f4f5"
              },
              "textColorPrimary": {
                "light": "#ff18181b",
                "dark": "#fff4f4f5"
              },
              "textColorPrimaryInverse": {
                "light": "#fff4f4f5",
                "dark": "#ff18181b"
              },
              "textColorSecondary": {
                "light": "#cc18181b",
                "dark": "#ccf4f4f5"
              },
              "textColorSecondaryInverse": {
                "light": "#ccf4f4f5",
                "dark": "#cc18181b"
              },
              "windowBackground": {
                "light": "#fff4f4f5",
                "dark": "#ff18181b"
              }
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6a32f563-3655-4ffb-97f7-45586f520c36"
      }
    },
    "owner": "icedcoffeeee"
  }
}
